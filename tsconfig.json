{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ES2015",
		"rootDir": "src",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"declaration": true,
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
